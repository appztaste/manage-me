{
  "name": "manageMe",
  "version": "1.0.0",
  "description": "A simple task manager",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "postinstall": "bower install",
    "start": "node app/index.js",
    "start-local": "http-server ./app -a 0.0.0.0 -p 8000",
    "test": "karma start spec/karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start spec/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "protractor": "protractor spec/protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rufusraghunath/manage-me"
  },
  "dependencies": {
    "bower": "1.7.7",
    "express": "~4.9.8"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rufusraghunath/manage-me"
  },
  "homepage": "https://github.com/rufusraghunath/manage-me#readme",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-uglify": "^1.0.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.5",
    "protractor": "^3.1.1"
  },
  "engines": {
    "node": "5.8.0"
  }
}
